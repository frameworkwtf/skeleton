{
    "name": "wtf/skeleton",
    "description": "Skeleton for WTF Framework",
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "monolog/monolog": "@stable",
        "wtf/auth": "^1",
        "wtf/core": "^1",
        "wtf/html": "^1",
        "wtf/orm": "^1.1",
        "nochso/html-compress-twig": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "discard-changes": true,
        "vendor-dir": "app/vendor"
    }
}
